{
  "name": "app-name",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "prepare": "husky",
    "clean:prebuild": "expo prebuild --clean",
    "build:android:debug": "eas build --platform android --profile debug",
    "build:android:local": "eas build --platform android --profile production --local --output ./build/app-name-$(date +'%Y-%m-%d-%H-%M-%S').aab --non-interactive",
    "build:android:production": "eas build --platform android --profile production --auto-submit",
    "build:ios:local": "eas build --platform ios --local --profile production --output ./build/app-name-$(date +'%Y-%m-%d-%H-%M-%S').ipa --non-interactive",
    "build:ios:local:interactive": "eas build --platform ios --local --profile production",
    "build:ios:production": "eas build --platform ios --profile production --auto-submit",
    "submit:ios": "eas submit -p ios --profile production",
    "submit:android": "eas submit -p android --profile production",
    "build:all:local": "npm run build:android:local && npm run build:ios:local",
    "task:all:local": "tsx ./scripts/task.ts -p all --output build --profile production --submit --local",
    "task:all:local:submit": "tsx ./scripts/task.ts -p all --profile production --submit",
    "task:all:remote": "tsx ./scripts/task.ts -p all --output build --profile production --submit",
    "task:android:local": "tsx ./scripts/task.ts -p android --output build --profile production --submit --local",
    "task:android:remote": "tsx ./scripts/task.ts -p android --output build --profile production --submit",
    "task:ios:local": "tsx ./scripts/task.ts -p ios --output build --profile production --submit --local",
    "task:ios:remote": "tsx ./scripts/task.ts -p ios --output build --profile production --submit",
    "clean:prebuild:ios": "expo prebuild --platform ios --clean",
    "clean:prebuild:android": "expo prebuild --platform android --clean",
    "clean:prebuild:all": "expo prebuild --clean"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.0",
    "@expo/vector-icons": "^14.0.2",
    "@hookform/resolvers": "^3.9.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/app": "^21.6.1",
    "@react-native-firebase/auth": "^21.6.1",
    "@react-native-firebase/firestore": "^21.6.1",
    "@react-native-firebase/storage": "^21.6.1",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/native": "^6.0.2",
    "@tamagui/config": "^1.108.4",
    "@tamagui/lucide-icons": "^1.108.4",
    "@tamagui/select": "^1.108.4",
    "@tanstack/query-async-storage-persister": "^5.62.2",
    "@tanstack/react-query": "^5.62.2",
    "@tanstack/react-query-persist-client": "^5.62.2",
    "burnt": "^0.12.2",
    "expo": "~51.0.39",
    "expo-apple-authentication": "~6.4.2",
    "expo-application": "~5.9.1",
    "expo-build-properties": "~0.12.5",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.9",
    "expo-haptics": "~13.0.1",
    "expo-notifications": "~0.28.17",
    "expo-image": "~1.13.0",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-local-authentication": "~14.0.1",
    "expo-router": "~3.5.24",
    "expo-secure-store": "~13.0.2",
    "expo-sharing": "^12.0.1",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "moment": "^2.30.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.2",
    "react-native": "0.74.5",
    "react-native-currency-input": "^1.1.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-pager-view": "6.3.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "tamagui": "^1.108.4",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/prompt-cli": "^19.4.0",
    "@commitlint/types": "^19.0.3",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "commander": "^12.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.10",
    "husky": "^9.1.5",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "~5.3.3"
  },
  "private": true
}
